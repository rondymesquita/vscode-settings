{
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "explorer.autoReveal": true,
  "editor.mouseWheelZoom": true,
  "telemetry.telemetryLevel": "off",
  "editor.fontFamily": "Dank Mono",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "editor.suggestOnTriggerCharacters": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "editor.hover.enabled": true,
  "workbench.sideBar.location": "left",
  "files.associations": {
    ".env": "properties",
    "*.env": "properties",
    "*.tsx": "typescriptreact"
  },
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontLigatures": true,
  "extensions.ignoreRecommendations": false,
  "editor.tabSize": 2,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "codesnap.showWindowControls": false,
  "codesnap.realLineNumbers": true,
  "editor.insertSpaces": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.openEditors.visible": 0,
  "workbench.editor.tabCloseButton": "off",
  "workbench.statusBar.visible": true,
  "breadcrumbs.enabled": true,
  "workbench.colorCustomizations": {
    // "editor.selectionBackground": "#641352",
    // "editor.wordHighlightBackground": "#12748a",
    // "editor.selectionHighlightBackground": "#135564",
    // "list.inactiveSelectionBackground": "#135564"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "editor.accessibilitySupport": "off",
  "files.eol": "\n",
  "explorer.confirmDelete": false,
  "debug.console.fontSize": 14,
  "terminal.integrated.fontSize": 14,
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "[typescript]": {
    "editor.defaultFormatter": "SimonSiefke.prettier-vscode"
  },
  "terminal.external.windowsExec": "C:\\Program Files\\Git\\bin\\bash.exe",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "explorer.compactFolders": false,
  "quokka.showCodeLensInOutputChannel": false,
  "mermaid-editor.generate.type": "png",
  "quokka.compactMessageOutput": true,
  "prettier.enable": false,
  "material-icon-theme.saturation": 1,
  "material-icon-theme.activeIconPack": "angular",
  "slidev.enabled": true,
  "editor.letterSpacing": -0.3,
  "terminal.integrated.letterSpacing": -0.1,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "[javascript]": {
    "editor.defaultFormatter": "SimonSiefke.prettier-vscode"
  },
  "workbench.colorTheme": "Dracula",
  "workbench.editor.decorations.badges": false,
  "workbench.editor.decorations.colors": false,
  "autoimport.autoComplete": false,
  "typescript.preferences.importModuleSpecifier": "relative",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[json]": {
    "editor.defaultFormatter": "SimonSiefke.prettier-vscode"
  }
}
