{
  "editor.mouseWheelZoom": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "editor.fontFamily": "Operator Mono",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
	"files.trimTrailingWhitespace": true,
  "javascript.suggest.enabled": false,
  "typescript.suggest.enabled": false,
	"workbench.sideBar.location": "left",
  "files.associations": {
    ".env": "shellscript",
    "*.env": "shellscript"
  },
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontLigatures": true,
  "extensions.ignoreRecommendations": false,
  "editor.tabSize": 2,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.minimap.enabled": false,
  "codesnap" : {
    "showWindowControls": false,
    "realLineNumbers": true
  },
	"editor.insertSpaces": false,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"explorer.openEditors.visible": 0,
	"workbench.editor.tabCloseButton": "off",
	"workbench.statusBar.visible": true,
	"breadcrumbs.enabled": true,
	"workbench.colorCustomizations": {
    "editor.selectionBackground": "#641352",
    "editor.wordHighlightBackground": "#12748a",
    "editor.selectionHighlightBackground": "#135564",
    "list.inactiveSelectionBackground": "#135564"
	},
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter.notebook.ipynb"
	},
  "editor.accessibilitySupport": "off",
  "files.eol": "\n",
  "explorer.confirmDelete": false,
  "markdown-preview-enhanced.liveUpdate": true,
  "workbench.colorTheme": "A Touch of Lilac Theme",
  "debug.console.fontSize": 14,
  "terminal.integrated.fontSize": 14,
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.external.windowsExec": "C:\\Program Files\\Git\\bin\\bash.exe",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "explorer.compactFolders": false,
  "quokka.showCodeLensInOutputChannel": false,
  "editor.formatOnSave": true,
  "window.zoomLevel": 1
}
