{
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  // "explorer.autoReveal": true,
  "editor.mouseWheelZoom": true,
  "telemetry.telemetryLevel": "off",
  "editor.fontFamily": "Cascadia Mono",
  "customizeUI.font.regular": "Cascadia Mono",
  "customizeUI.font.monospace": "Cascadia Mono",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "editor.suggestOnTriggerCharacters": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "editor.hover.enabled": true,
  "files.associations": {
    ".env": "properties",
    "*.env": "properties",
    "*.tsx": "typescriptreact"
  },
  "editor.fontLigatures": true,
  "extensions.ignoreRecommendations": false,
  "editor.tabSize": 2,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "codesnap.showWindowControls": false,
  "codesnap.realLineNumbers": true,
  "editor.insertSpaces": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.openEditors.visible": 0,
  "workbench.editor.tabCloseButton": "off",
  "breadcrumbs.enabled": true,
  "workbench.colorCustomizations": {},
  // "workbench.editorAssociations": {
  //   "*.ipynb": "jupyter.notebook.ipynb"
  // },
  "editor.accessibilitySupport": "off",
  "files.eol": "\n",
  "explorer.confirmDelete": false,
  "debug.console.fontSize": 12,
  "terminal.integrated.fontSize": 12,
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  // "[typescript]": {
  //   "editor.defaultFormatter": "SimonSiefke.prettier-vscode"
  // },
  "terminal.external.windowsExec": "C:\\Program Files\\Git\\bin\\bash.exe",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "explorer.compactFolders": false,
  "quokka.showCodeLensInOutputChannel": false,
  "mermaid-editor.generate.type": "png",
  "mermaid-export.outputType": "pdf",
  "quokka.compactMessageOutput": true,
  "prettier.enable": false,
  "slidev.enabled": true,
  "editor.letterSpacing": -0.3,
  "terminal.integrated.letterSpacing": -0.1,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  // "[javascript]": {
  //   "editor.defaultFormatter": "SimonSiefke.prettier-vscode"
  // },
  "workbench.colorTheme": "Night Owl",
  "workbench.editor.decorations.badges": false,
  "autoimport.autoComplete": false,
  "typescript.preferences.importModuleSpecifier": "relative",
  // "editor.defaultFormatter": "esbenp.prettier-vscode",
  // "[json]": {
  //   "editor.defaultFormatter": "SimonSiefke.prettier-vscode"
  // },
  "editor.formatOnSave": true,
  "prettier.documentSelectors": ["**/*.jsx", "**/*.json"],
  "workbench.editor.decorations.colors": false,
  "markdown.preview.scrollPreviewWithEditor": false,
  "markdown.preview.scrollEditorWithPreview": false,
  "markdown-preview-enhanced.scrollSync": false,
  "markdown-preview-enhanced.previewTheme": "github-light.css",
  "[typescript]": {
    "editor.defaultFormatter": "svipas.prettier-plus"
  },
  "workbench.iconTheme": "vscode-great-icons",
  "autoDayNightThemeSwitcher.autoToggleTimeNightBegin": "12:00",
  "autoDayNightThemeSwitcher.autoToggleTimeNightEnd": "7:00",
  "autoDayNightThemeSwitcher.dayTheme": "Github Light",
  "autoDayNightThemeSwitcher.nightTheme": "Github Dark",
  "autoDayNightThemeSwitcher.dayThemeCustomizations": {},
  "terminal.integrated.persistentSessionScrollback": 1000
}
